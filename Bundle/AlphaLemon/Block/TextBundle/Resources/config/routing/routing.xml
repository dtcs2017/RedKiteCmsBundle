<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="_create_images_list" pattern="/backend/{_locale}/al_createImagesList">
        <default key="_controller">TextBundle:TinyMCE:createImagesList</default>
        <default key="_locale">en</default>
    </route>
    <route id="_create_links_list" pattern="/backend/{_locale}/al_createPermalinksList/{language}">
        <default key="_controller">TextBundle:TinyMCE:createLinksList</default>
        <default key="_locale">en</default>
    </route>
</routes>
