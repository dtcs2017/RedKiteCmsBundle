<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="app_navigation_menu.block.class">AlphaLemon\AlphaLemonCmsBundle\Core\Bundles\NavigationMenuBundle\Core\Block\AlBlockManagerNavigationMenu</parameter>
    </parameters>

    <services>
        <service id="app_navigationmenu.block" class="%app_navigation_menu.block.class%">
            <argument type="service" id="service_container" />
            <tag name="alphalemon_cms.blocks_factory.block" description="Navigation menu block" type="NavigationMenu" group="alphalemon_internals" />
        </service>
    </services>
</container>