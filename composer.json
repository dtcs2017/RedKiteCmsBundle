{
  "name": "alphalemon/alphalemon-cms-bundle",
  "description": "AlphaLemon CMS is an Open Source Content Management System Application, built on top of Symfony2 Framework, providing an easy an intuitive user-interface, to improve the website's content management experience",
  "keywords": ["alphalemon", "cms"],
  "homepage": "http://alphalemon.com",
  "type": "symfony-bundle",
  "license": "GPLv2",
  "authors": [
    {
      "name": "AlphaLemon",
      "email": "info@alphalemon.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/symfony": ">=2.0.12",
    "doctrine/orm": ">=2.1.0",
    "twig/extensions": "*",

    "symfony/assetic-bundle": "2.0.*",
    "sensio/generator-bundle": "2.0.*",
    "sensio/framework-extra-bundle": "2.0.*",
    "sensio/distribution-bundle": "2.0.*",
    "jms/security-extra-bundle": "1.0.*",
    "imagine/Imagine": ">=0.2",
    "alphalemon/theme-engine-bundle": "dev-master",
    "alphalemon/frontend-bundle": "dev-master",
    "alphalemon/elfinder-bundle": "dev-master",
    "alphalemon/business-website-theme-bundle": "dev-master"
  },
  "autoload": {
    "psr-0": {
        "AlphaLemon\\AlphaLemonCmsBundle": "",
        'AlphaLemon\\AlphaLemonCmsBundle\\Core\\Bundles\\TextBundle' : "",
        'AlphaLemon\\AlphaLemonCmsBundle\\Core\\Bundles\\MenuBundle' : "",
        'AlphaLemon\\AlphaLemonCmsBundle\\Core\\Bundles\\MediaBundle' : "",
        'AlphaLemon\\AlphaLemonCmsBundle\\Core\\Bundles\\ScriptBundle' : "",
        'AlphaLemon\\AlphaLemonCmsBundle\\Core\\Bundles\\NavigationMenuBundle' : ""
    }
  },
  "target-dir" : "AlphaLemon/AlphaLemonCmsBundle",
  "bin": ["bin/cms-install", "bin/config.php"],
  "config": {
    "bin-dir": "bin"
  }
}